<document
  xmlns="http://cnx.rice.edu/cnxml"
  xmlns:md="http://cnx.rice.edu/mdml"
  xmlns:m="http://www.w3.org/1998/Math/MathML"
  id="imported-from-openoffice"
  module-id="imported-from-openoffice"
  cnxml-version="0.7"
  class="introduction">

  <title>Test Introduction Module</title>
  <metadata xmlns:md="http://cnx.rice.edu/mdml">
    <md:version>1.0</md:version>
    <md:actors>
      <md:person userid="testid">
        <md:firstname>TestFirstname</md:firstname>
        <md:surname>TestSurname</md:surname>
        <md:fullname>TestFullname</md:fullname>
        <md:email>testemail@cnx.org</md:email>
      </md:person>
    </md:actors>
    <md:roles>
      <md:role type="author">testid</md:role>
      <md:role type="maintainer">testid</md:role>
      <md:role type="licensor">testid</md:role>
    </md:roles>
    <md:license url="http://creativecommons.org/licenses/by/3.0/"/>
  </metadata>
  <content>

    <!-- End boilerplate -->

    <note class="howto">
      <title>Test howto Note</title>
      <para>Lorem Ipsum</para>
    </note>

    <note class="qa">
      <title>Test qa Note</title>
      <para>Lorem Ipsum</para>
      <figure>
        <title>QA Figure Title</title>
      </figure>
    </note>


    <note class="media">
      <title>Test media Note</title>
      <para>Lorem Ipsum</para>
      <list>
        <item>
          <link class="only-show-once" url="http://cnx.org">Only once link</link>
        </item>
        <item>
          <link url="http://cnx.org">Plain Link</link>
        </item>
      </list>
    </note>

    <note class="try">
      <title>Test try Note</title>
      <para>Lorem Ipsum</para>
      <exercise>
        <title>Test exercise Title</title>
        <problem>Test Problem</problem>
        <solution>Test Solution</solution>
      </exercise>
    </note>


  </content>
